{
  "description": "Maestro Orchestrator hooks for server integration",
  "hooks": {
    "SessionStart": [
      {
        "matcher": "*",
        "hooks": [
          {
            "type": "command",
            "command": "maestro session register",
            "timeout": 5
          }
        ]
      }
    ],
    "SessionEnd": [
      {
        "matcher": "*",
        "hooks": [
          {
            "type": "command",
            "command": "maestro session complete",
            "timeout": 3
          }
        ]
      }
    ]
  }
}
