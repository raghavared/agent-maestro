{
  "$schema": "https://schema.tauri.app/config/2/capability",
  "identifier": "default",
  "description": "Default capabilities for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:event:allow-emit",
    "core:event:allow-listen",
    "core:webview:default",
    "core:webview:allow-webview-close",
    "core:window:default",
    "shell:allow-open",
    "drag:default",
    "dialog:allow-save",
    "dialog:allow-open"
  ]
}
